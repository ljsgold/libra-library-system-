{
  "buildCommand": "cd libra-frontend && npm install && npm run build",
  "outputDirectory": "libra-frontend/dist",
  "framework": "vite",
  "installCommand": "cd libra-frontend && npm install",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "http://your-backend-server-url/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}